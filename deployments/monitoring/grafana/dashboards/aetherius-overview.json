{
  "dashboard": {
    "title": "Aetherius - System Overview",
    "tags": ["aetherius", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "up{job=~\"agent-manager|orchestrator-service|reasoning-service|collect-agent\"}",
            "legendFormat": "{{ job }}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "Down"},
              {"type": "value", "value": "1", "text": "Up"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Total Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (job)",
            "legendFormat": "{{ job }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (job)",
            "legendFormat": "{{ job }} - 5xx",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m])) by (job)",
            "legendFormat": "{{ job }} - 4xx",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "API Response Time (P95)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (job, le))",
            "legendFormat": "{{ job }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Agent Registration",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(agent_registered_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Active Workflows",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(workflow_executions_active)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "title": "Events Processed (Last 1h)",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(increase(events_processed_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Root Cause Analysis Accuracy",
        "type": "gauge",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "targets": [
          {
            "expr": "avg(analysis_accuracy_ratio)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=~\"agent-manager|orchestrator-service|reasoning-service\"} / 1024 / 1024",
            "legendFormat": "{{ job }} - {{ pod }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "mbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=~\"agent-manager|orchestrator-service|reasoning-service\"}[5m]) * 100",
            "legendFormat": "{{ job }} - {{ pod }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU %"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Goroutines",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "go_goroutines{job=~\"agent-manager|orchestrator-service\"}",
            "legendFormat": "{{ job }} - {{ pod }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Goroutines"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "NATS Message Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "rate(nats_server_in_msgs[5m])",
            "legendFormat": "In Messages",
            "refId": "A"
          },
          {
            "expr": "rate(nats_server_out_msgs[5m])",
            "legendFormat": "Out Messages",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}